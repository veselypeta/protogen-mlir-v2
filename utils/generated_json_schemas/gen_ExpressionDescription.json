{
    "$ref": "#/definitions/ExpressionDescription",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "BinaryExpr": {
            "properties": {
                "lhs": {
                    "$ref": "#/definitions/ExpressionDescription",
                    "title": "lhs"
                },
                "op": {
                    "$ref": "#/definitions/BinaryOp",
                    "title": "op"
                },
                "rhs": {
                    "$ref": "#/definitions/ExpressionDescription",
                    "title": "rhs"
                }
            },
            "required": [
                "lhs",
                "op",
                "rhs"
            ],
            "title": "BinaryExpr",
            "type": "object"
        },
        "BinaryOp": {
            "enum": [
                "!=",
                "&",
                "*",
                "+",
                "-",
                "->",
                "/",
                "<",
                "<=",
                "=",
                ">",
                ">="
            ],
            "title": "BinaryOp",
            "type": "string"
        },
        "Designator": {
            "properties": {
                "id": {
                    "title": "id",
                    "type": "string"
                },
                "indexes": {
                    "items": {
                        "$ref": "#/definitions/Indexer"
                    },
                    "title": "indexes",
                    "type": "array"
                }
            },
            "required": [
                "id",
                "indexes"
            ],
            "title": "Designator",
            "type": "object"
        },
        "Expression": {
            "anyOf": [
                {
                    "$ref": "#/definitions/ProcCall"
                },
                {
                    "$ref": "#/definitions/Designator"
                },
                {
                    "$ref": "#/definitions/BinaryExpr"
                },
                {
                    "$ref": "#/definitions/NegExpr"
                },
                {
                    "$ref": "#/definitions/MultisetCount"
                },
                {
                    "type": "string"
                }
            ],
            "title": "Expression"
        },
        "ExpressionDescription": {
            "properties": {
                "expression": {
                    "$ref": "#/definitions/Expression",
                    "title": "expression"
                },
                "typeId": {
                    "$ref": "#/definitions/ExpressionType",
                    "title": "typeId"
                }
            },
            "required": [
                "expression",
                "typeId"
            ],
            "title": "ExpressionDescription",
            "type": "object"
        },
        "ExpressionType": {
            "enum": [
                "ID",
                "binary",
                "designator",
                "ms_count",
                "neg_expr",
                "proc_call"
            ],
            "title": "ExpressionType",
            "type": "string"
        },
        "Indexer": {
            "properties": {
                "index": {
                    "$ref": "#/definitions/ExpressionDescription",
                    "title": "index"
                },
                "typeId": {
                    "enum": [
                        "array",
                        "object"
                    ],
                    "title": "typeId",
                    "type": "string"
                }
            },
            "required": [
                "index",
                "typeId"
            ],
            "title": "Indexer",
            "type": "object"
        },
        "MultisetCount": {
            "properties": {
                "predicate": {
                    "$ref": "#/definitions/ExpressionDescription",
                    "title": "predicate"
                },
                "varId": {
                    "title": "varId",
                    "type": "string"
                },
                "varValue": {
                    "$ref": "#/definitions/ExpressionDescription",
                    "title": "varValue"
                }
            },
            "required": [
                "predicate",
                "varId",
                "varValue"
            ],
            "title": "MultisetCount",
            "type": "object"
        },
        "NegExpr": {
            "properties": {
                "expr": {
                    "$ref": "#/definitions/ExpressionDescription",
                    "title": "expr"
                }
            },
            "required": [
                "expr"
            ],
            "title": "NegExpr",
            "type": "object"
        },
        "ProcCall": {
            "properties": {
                "actuals": {
                    "items": {
                        "$ref": "#/definitions/ExpressionDescription"
                    },
                    "title": "actuals",
                    "type": "array"
                },
                "funId": {
                    "title": "funId",
                    "type": "string"
                }
            },
            "required": [
                "actuals",
                "funId"
            ],
            "title": "ProcCall",
            "type": "object"
        }
    }
}

